{
  "name": "Excalidraw",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "migrate:dev": "pnpm run  prisma migrate dev --schema packages/db/prisma/schema.prisma",
    "migrate:reset": "pnpm run prisma migreate reset --schema packages/db/prisma/schema.prisma"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "prisma": "^6.17.1"
  }
}
