{
  "name": "@excalidraw/types",
  "version": "1.0.0",
  "description": "Shared types and schemas for the monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "types": "./dist/schemas.d.ts"
    }
  }
}
